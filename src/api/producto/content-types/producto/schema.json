{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "descripcion": {
      "type": "text",
      "required": false,
      "unique": true
    },
    "cantidad": {
      "type": "biginteger",
      "required": true
    },
    "categoria": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::categoria.categoria",
      "mappedBy": "producto"
    },
    "Precio": {
      "type": "biginteger",
      "required": true
    }
  }
}
